nextflow_process {

    name "Test Process PVACTOOLS_INSTALLVEPPLUGIN"
    script "../main.nf"
    process "PVACTOOLS_INSTALLVEPPLUGIN"

    tag "modules"
    tag "modules_nfcore"
    tag "pvactools"
    tag "pvactools/installvepplugin"

    test("test-pvactools-installvepplugins") {

        when {
            process {}
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }

    }

    test("test-pvactools-installvepplugins - stub") {
        options "-stub"
        when {
            process {}
        }

        then {
            assertAll(
                { assert process.success },
            )
        }
    }
}