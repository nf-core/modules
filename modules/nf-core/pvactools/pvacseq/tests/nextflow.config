process {
    cpus   = 2
    memory = '8.GB'
    time   = '2.h'

    withName: 'PVACTOOLS_PVACSEQ' {
        ext.args = "-e1 9 -e2 15"
    }

    withName: 'ENSEMBLVEP_VEP' {
        ext.args   = "--format vcf --vcf --symbol --terms SO --tsl --biotype --hgvs --cache --plugin Frameshift --plugin Wildtype --dir_plugins VEP_plugins"
        ext.prefix = { "${meta.id}_vep" }
    }
}
