        tag "stub"
        options '-stub'
        when {
            process {
                """
                input[0] = [[id : "test"], []]
                input[1] = file("https://cf.10xgenomics.com/supp/cell-arc/cellranger-arc-tiny-bcl-atac-simple-1.0.0.csv", checkIfExists:true)
