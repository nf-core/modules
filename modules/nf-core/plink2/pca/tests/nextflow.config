/*
========================================================================================
    Nextflow config file for running tests
========================================================================================
*/
process {

    withName: PLINK2_VCF {
        ext.args = '--make-pgen --set-missing-var-ids @:#:\\$1:\\$2'
    }

    withName: PLINK2_PCA {
        ext.prefix = { "${meta.id}.pca" }
    }
}
