nextflow_process {

    name "Test Process CRISPRESSO2"
    script "../main.nf"
    process "CRISPRESSO2"

    tag "modules"
    tag "modules_nfcore"  
    tag "crispresso2"

    test("crispresso2 test") {
        
        options "-stub"
        
        when {
            process {
                """
                input[0] = [
                    [ id:'test', single_end:true ],
                    []
                ]
                """
            }
        }
        
        then {
            assert process.success
            assert snapshot(process.out).match()
        }
    }
}
