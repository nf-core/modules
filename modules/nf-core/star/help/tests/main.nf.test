nextflow_process {

    name "Test Process STAR_HELP"
    script "../main.nf"
    process "STAR_HELP"

    tag "modules"
    tag "modules_nfcore"
    tag "star"
    tag "star/help"

    test("normal run") {

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }

    }

    test("stub run") {

        options "-stub"

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }

    }

}
