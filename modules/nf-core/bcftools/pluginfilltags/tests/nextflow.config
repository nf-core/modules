process {
    withName: "BCFTOOLS_PLUGINFILLTAGS" {
        ext.args = params.bcftools_pluginfilltags_args
        ext.args2 = params.bcftools_pluginfilltags_args2
    }

    withName: "BCFTOOLS_QUERY" {
        ext.args = "--list-samples"
    }

    withName: "GAWK" {
        ext.prefix = { "${meta.id}_sample" }
    }
}
