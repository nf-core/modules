params{
    // Optional parameters for GMM-demux - Default values
    threshold = 0.8
    full_report = 'test'
    num_cells = 35685
    output_path = 'test'
    examine = " example_cell_types.txt"
    ambiguous = 0.05

}

process {
    withName: GMMDEMUX {
        ext.args = "-f ${params.full_report} -t ${params.threshold} --summary ${params.num_cells} -o ${params.output_path} -e ${params.examine} -a ${params.ambiguous} "
    }
}
