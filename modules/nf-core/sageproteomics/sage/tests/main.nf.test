nextflow_process {

    name "Test Process SAGEPROTEOMICS_SAGE"
    script "../main.nf"
    process "SAGEPROTEOMICS_SAGE"

    tag "modules"
    tag "modules_nfcore"
    tag "sageproteomics"
    tag "sageproteomics/sage"

    test("sageproteomics - mzML") {
        when {
            process {
                """
		input[0] = Channel.of([
                    [ id:'test'], // meta map
                    file(params.test_data['proteomics']['msspectra']['profile_spectra'], checkIfExists: true)
	        ])
		input[1] = file(params.test_data['proteomics']['database']['yeast_ups'], checkIfExists: true)
		input[2] = file('https://raw.githubusercontent.com/lazear/sage/master/tests/config-cli.json')
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out.results_pin, process.out.results_tsv, process.out.versions).match() }
            )
        }

    }
    test("sageproteomics - mzML.gz - stub") {
        options "-stub"
        when {
            process {
                """
                // TODO nf-core: define inputs of the process here. Example:
                		input[0] = Channel.of([
                    [ id:'test'], // meta map
                    file(params.test_data['proteomics']['msspectra']['profile_spectra'], checkIfExists: true)
	        ])
		input[1] = file(params.test_data['proteomics']['database']['yeast_ups'], checkIfExists: true)
		input[2] = file('https://raw.githubusercontent.com/lazear/sage/master/tests/config-cli.json')
                """
            }
        }
        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }

    }

}
