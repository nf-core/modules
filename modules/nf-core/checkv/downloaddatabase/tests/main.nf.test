nextflow_process {

    name "Test Process CHECKV_DOWNLOADDATABASE"
    script "../main.nf"
    process "CHECKV_DOWNLOADDATABASE"
    options "-stub"

    tag "modules"
    tag "modules_nfcore"
    tag "checkv"
    tag "checkv/downloaddatabase"

    test("No input: download only") {

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }

    }

}
