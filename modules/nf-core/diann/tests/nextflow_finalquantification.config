// Configuration for final quantification mode
// Matches the subworkflow DIANN_FINALQUANTIFICATION configuration
// Note: Includes --matrix-qvalue 1 which is test-specific (not in base subworkflow config)
process {
    withName: 'DIANN' {
        ext.prefix = 'diann_report'
        ext.args = '--verbose 3 --relaxed-prot-inf --pg-level 1 --use-quant --matrices --qvalue 1.0 --matrix-qvalue 1 --individual-windows'
    }
}
