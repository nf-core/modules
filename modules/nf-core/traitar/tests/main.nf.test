// nf-core modules test traitar
nextflow_process {

    name "Test Process TRAITAR"
    script "../main.nf"
    process "TRAITAR"

    tag "modules"
    tag "modules_nfcore"
    tag "traitar"

    test("traitar - proteomes - stub") {

        options "-stub"

        when {
            process {
                """
                input[0] = [
                    [ id:'test_sample' ],
                    file(params.modules_testdata_base_path + 'genomics/prokaryotes/bacteroides_fragilis/proteome/proteome.faa', checkIfExists: false),
                ]
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }

    }

}
