nextflow_process {

    name "Test Process TRAITAR_PFAMGET"
    script "../main.nf"
    process "TRAITAR_PFAMGET"

    tag "modules"
    tag "modules_nfcore"
    tag "traitar"
    tag "traitar/pfamget"

    test("Download PFAM database - default version") {

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }
    }

    test("Download PFAM database - default version - stub") {

        options "-stub"

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }
    }
}
