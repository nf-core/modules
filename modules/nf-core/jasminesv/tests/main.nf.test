nextflow_process {

    name "Test Process JASMINESV"
    script "../main.nf"
    process "JASMINESV"

    tag "modules"
    tag "modules_nfcore"
    tag "jasminesv"

    config "./nextflow.config"

    test("homo_sapiens - [vcf, vcf2], [], [], [], [], [] - vcf") {

        when {
            process {
                """
                input[0] = [
                    [ id:"test" ],
                    [
                        file(params.modules_testdata_base_path + "genomics/homo_sapiens/illumina/gvcf/test.genome.vcf", checkIfExists:true),
                        file(params.modules_testdata_base_path + "genomics/homo_sapiens/illumina/gvcf/test2.genome.vcf", checkIfExists:true),
                    ],
                    [],
                    []
                ]
                input[1] = [[],[]]
                input[2] = [[],[]]
                input[3] = []
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(
                    process.out.collectEntries { key, val ->
                        if(key.matches("\\d+")) {
                            // Skip the integer keys
                            return null
                        }
                        if(key == "vcf") {
                            return [key, val.collect { it.collect { it instanceof Map ? it : "${file(it).name},variantsMD5:${path(it).vcf.variantsMD5}" } }]
                        }
                        return [key, val]
                    }.findAll { it != null }
                ).match() }
            )
        }

    }

