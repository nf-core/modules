nextflow_process {

    name "Test Process SCIMAP_SPATIALLDA"
    script "../main.nf"
    config "./nextflow.config"
    process "SCIMAP_SPATIALLDA"

    tag "modules"
    tag "modules_nfcore"
    tag "scimap"
    tag "scimap/spatiallda"

    test("phenotyped - csv") {
        when {
            process {
                """
                input[0] = [
                    [ id:'test' ],
                    file(params.test_data['imaging']['downstream']['phenotyped'], checkIfExists: true)
                ]
                """
            }
        }

        then {
            assertAll(
                { assert process.success }
            )
        }
    }

}
