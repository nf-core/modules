process {

    publishDir = [
        mode: "copy",
        path: { "${params.outdir}/${task.process.tokenize(':')[-1].tokenize('_')[0].toLowerCase()}" },
    ]
    withName: 'IGV_JS' {
        ext.args = """genome: "hg38",
                locus: "chr22:4,514-4,554""""
        ext.args2 = """"format": "bam",
                        "type": "alignment""""
    }

}
